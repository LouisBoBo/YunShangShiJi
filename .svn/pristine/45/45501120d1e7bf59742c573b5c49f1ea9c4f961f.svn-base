{{if(!!it&&it.length>0){}}
	{{for(var i=0;i<it.length;i++){}}
	<div class="col-xs-6" is_show="{{=it[i].is_show}}">
		<div class="goodsitem" data-shop_code="{{=it[i].shop_code}}">
			<table>
				<tr>
					<td valign="top" class="goodsimg">
						<div class="relative">
							<div class="isTop" onclick="toTop(this)" is_show="{{=it[i].is_show}}" data-shop_code="{{=it[i].shop_code}}">
								{{if(it[i].is_show=="0"){}}
								<img src="https://yssj668.b0.upaiyun.com/H5/public/img/isTop.png">
								{{}else{}}
								<img src="https://yssj668.b0.upaiyun.com/H5/public/img/notTop.png">
								{{}}}
							</div>
							<a><img class="fadeIn" src="{{=youpai_url}}{{=it[i].def_pic}}!280" width="78"></a>
						</div>
						
					</td>
					<td valign="top">
						<div class="fs16" onclick="toGoodsDetail(this)" data-shop_code="{{=it[i].shop_code}}" data-shop_name="{{=it[i].shop_name}}">
							<span>{{=it[i].shop_name}}</span>
						</div>
						<div class="clearfix">
							<div class="pull-right">
								<a><span class="{{if(it[i].isLike==1){}} icon-liked {{}else{}} icon-like {{}}}"></span></a>&nbsp;
								<a><span class="{{if(it[i].isCart==1){}} icon-shopcarted {{}else{}} icon-shopcart2 {{}}}"></span></a>
							</div>
							<div>
								<span>￥{{=it[i].shop_se_price}}</span>
							</div>
						</div>
						{{if(!!it[i].content){}}
						<div>
							<p class="gray">{{=it[i].content}}</p>
						</div>
						{{}}}
						{{if(!!it[i].shop_pic){}}
						<div class="container-fluid" onclick="toGoodsDetail(this)" data-shop_code="{{=it[i].shop_code}}" data-shop_name="{{=it[i].shop_name}}">
							<div class="row">
								{{var four_picArr=it[i].shop_pic.split(",");for(var j=0;j<four_picArr.length;j++){}}
									{{if(four_picArr[j].indexOf("revea_")>-1||four_picArr[j].indexOf("detail_")>-1||four_picArr[j].indexOf("real_")>-1){}}
										<div class="col-xs-4">
											<div class="squareDiv"><img src="{{=(youpai_url+four_picArr[j]+'!280')}}" width="100%"></div>
										</div>
									{{}}}
								{{}}}
							</div>
						</div>
						{{}}}
						<div class="clearifx">
							<div class="pull-right">
								<span class="icon-chat2" onclick="toChat2(this)"></span>
							</div>
							<div>
								<span class="gray">上新时间 {{=(new Date(it[i].add_time)).format("yyyy-MM-dd hh:mm:ss")}}</span>
							</div>
						</div>
					</td>
				</tr>
			</table>
			
		</div>
	</div>
	{{}}}
{{}else{}}
	
{{}}}
